{
  "name": "@sparkbox/carbon-cli",
  "version": "0.1.0",
  "description": "Project Setup Automation",
  "keywords": [
    "clone",
    "project",
    "template",
    "generator",
    "config"
  ],
  "author": "Sparkbox",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sparkbox/carbon-cli.git"
  },
  "bugs": {
    "url": "https://github.com/sparkbox/carbon-cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "cp": "dist/main.js"
  },
  "files": [
    "dist/",
    "man/carbon.1"
  ],
  "man": [
    "man/carbon.1"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "npm run build -- -w",
    "docs": "ronn man/carbon.1.ronn",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "rm -rf dist/",
    "prepublishOnly": "npm run build && npm t && npm run docs",
    "release": "standard-version",
    "test": "ava"
  },
  "devDependencies": {
    "@types/ejs": "^2.6.3",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.7.3",
    "@types/shelljs": "^0.8.5",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "ava": "^2.3.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "@octokit/rest": "^16.28.9",
    "boxen": "^4.1.0",
    "chalk": "^2.4.2",
    "ejs": "^2.6.2",
    "fuzzy": "^0.1.3",
    "inquirer": "^7.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "ora": "^3.4.0",
    "shelljs": "^0.8.3"
  },
  "engines": {
    "node": ">=10"
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/mock"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
