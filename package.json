{
  "name": "@sparkbox/carbon-cli",
  "version": "0.2.2",
  "description": "Project Setup Automation",
  "keywords": [
    "clone",
    "project",
    "template",
    "generator",
    "config"
  ],
  "author": "Sparkbox",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sparkbox/carbon-cli.git"
  },
  "bugs": {
    "url": "https://github.com/sparkbox/carbon-cli/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "carbon": "dist/main.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "coverage": "nyc --reporter=lcov ava",
    "dev": "npm run build -- -w",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "rm -rf dist/",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "test": "ava"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/ejs": "^2.6.3",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.7.3",
    "@types/rewire": "^2.5.28",
    "@types/shelljs": "^0.8.5",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "ava": "^2.3.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "execa": "^2.0.4",
    "mock-fs": "^4.10.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rewiremock": "^3.13.7",
    "sinon": "^7.5.0",
    "standard-version": "^7.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "@octokit/rest": "^16.28.9",
    "boxen": "^4.1.0",
    "chalk": "^2.4.2",
    "ejs": "^2.6.2",
    "fuzzy": "^0.1.3",
    "inquirer": "^7.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "ora": "^3.4.0",
    "shelljs": "^0.8.3"
  },
  "engines": {
    "node": ">=10"
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/mocks"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ]
  }
}
